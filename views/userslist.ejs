<%- include('./base/header.ejs') %>
<div class="container"></div>
    <div class="row" style="width:auto;">
        <h4 class="text-centre my-3" style="text-align: center;">Users List</h4>
        <table class="table-text-centre" style="text-align: center;">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Country</th>
                    <th>Email</th>
                    <th>Mobile No</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <% users.forEach((users)=>{%>
                <tr>
                    <td><%= users.firstName%></td>
                    <td><%= users.lastName%></td>
                    <td><%= users.country%></td>
                    <td><%= users.email%></td>
                    <td><%= users.mobileNo%></td>
                    <td>
                        <a href="/edit/user/page/<%= users._id %>" class="btn btn-success rounded-1">Edit</a>
                        <a href="/delete/user/page/<%= users._id %>" class="btn btn-danger rounded-1">Delete</a>
                    </td>
                </tr>
           <% }) %>
        </table>
    </div>
</div>
<%- include('./base/footer.ejs') %>